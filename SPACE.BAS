TYPE ShipClass
  Callsign AS STRING * 10
  CallCol AS INTEGER
  X AS INTEGER
  Y AS INTEGER
  W AS INTEGER
  H AS INTEGER
  Health AS INTEGER
END TYPE

TYPE PosClass
  X AS INTEGER
  Y AS INTEGER
END TYPE

TYPE ExpClass
  FrameState AS INTEGER
  X AS INTEGER
  Y AS INTEGER
END TYPE

REM Set Dimensions
CONST ShipTexX = 15
CONST ShipTexY = 19
CONST EnemyTex1X = 15
CONST EnemyTex1Y = 19
CONST MaxNumEnemies = 3
CONST NumExplosionFrames = 9, MaxNumExplosions = 10
CONST NumSmokeFrames = 7, MaxNumSmoke = 10

DIM SHARED Ship AS ShipClass
DIM SHARED Enemy1(MaxNumEnemies) AS ShipClass
DIM SHARED ShipTex(ShipTexX, ShipTexY) AS INTEGER
DIM SHARED EnemyTex1(EnemyTex1X, EnemyTex1Y) AS INTEGER
DIM SHARED ExplosionTex(NumExplosionFrames, ShipTexX, ShipTexY) AS INTEGER
DIM SHARED SmokeTex(NumSmokeFrames, ShipTexX, ShipTexY) AS INTEGER
DIM SHARED Old AS INTEGER
DIM SHARED Starfield1(30) AS PosClass
DIM SHARED Starfield2(30) AS PosClass
DIM SHARED Bullet(30) AS PosClass
DIM SHARED Explosion(MaxNumExplosions) AS ExpClass
DIM SHARED Smoke(MaxNumSmoke) AS ExpClass
DIM SHARED Score AS INTEGER

RANDOMIZE TIMER
SCREEN 12
CALL Init
CALL MainMenu
END

SUB Background1
  FOR a = 0 TO 30
    Starfield1(a).Y = Starfield1(a).Y + 1
    IF Starfield1(a).Y > 480 THEN
      Starfield1(a).Y = 1
      Starfield1(a).X = INT(RND * 639) + 1
    END IF
    PSET (Starfield1(a).X, Starfield1(a).Y), 15
    PSET (Starfield1(a).X, Starfield1(a).Y - 1), 7
    PSET (Starfield1(a).X, Starfield1(a).Y - 2), 8
    PSET (Starfield1(a).X, Starfield1(a).Y - 3), 0
  NEXT a
END SUB

SUB Background2
  STATIC LastUpdate
  Now = TIMER
  FOR a = 0 TO 30
    PSET (Starfield2(a).X, Starfield2(a).Y), 15
    PSET (Starfield2(a).X, Starfield2(a).Y - 1), 7
    PSET (Starfield2(a).X, Starfield2(a).Y - 2), 8
    PSET (Starfield2(a).X, Starfield2(a).Y - 3), 0
    PSET (Starfield2(a).X, Starfield2(a).Y - 4), 0
  NEXT a
  IF (Now - LastUpdate >= .01) THEN
    FOR b = 0 TO 30
      Starfield2(b).Y = Starfield2(b).Y + 2
      IF Starfield2(b).Y > 480 THEN
        Starfield2(b).Y = 1
        Starfield2(b).X = INT(RND * 639) + 1
      END IF
    NEXT b
    LastUpdate = Now
  END IF
END SUB

SUB Boom (Enemy)
  STATIC Index
  Explosion(Index).X = Enemy1(Enemy).X
  Explosion(Index).Y = Enemy1(Enemy).Y
  Explosion(Index).FrameState = 1
  Index = Index + 1
  IF Index >= MaxNumExplosions THEN
    Index = 0
  END IF
END SUB

SUB BoomSmoke (PosIndex)
  STATIC Index
  Smoke(Index).X = Explosion(PosIndex).X
  Smoke(Index).Y = Explosion(PosIndex).Y
  Smoke(Index).FrameState = 1
  Index = Index + 1
  IF Index >= MaxNumSmoke THEN
    Index = 0
  END IF
END SUB

SUB Damage (Amount)
  IF Ship.Health - Amount <= 0 THEN
    Done = 1
  ELSE
    Ship.Health = Ship.Health - Amount
  END IF
END SUB

SUB GameLoop
  CLS
  DO
    KeyPress$ = INKEY$
    KeyPress$ = UCASE$(KeyPress$)
    SELECT CASE KeyPress$
    CASE "W"
      CALL RenderShip(0, -10)
    CASE "S"
      CALL RenderShip(0, 10)
    CASE "A"
      CALL RenderShip(-10, 0)
    CASE "D"
      CALL RenderShip(10, 0)
    CASE "Q"
      CALL RenderShip(-10, -10)
    CASE "E"
      CALL RenderShip(10, -10)
    CASE "Z"
      CALL RenderShip(-10, 10)
    CASE "C"
      CALL RenderShip(10, 10)
    CASE " "
      CALL Shoot
    CASE ELSE
      CALL Background1
      CALL Background2
      CALL UpdateBullet
      CALL TimeUpdateShip
      CALL TimeUpdateEnemy
      CALL TimeUpdateExplosion
      CALL TimeUpdateSmoke
      CALL RenderHealth
      CALL Damage(1)
      LOCATE 2, 1: PRINT Score
    END SELECT
  LOOP UNTIL KeyPress$ = CHR$(27)
END SUB

SUB Init
  Score = 0
  FOR a = 0 TO ShipTexY
    FOR b = 0 TO ShipTexX
      READ ShipTex(b, a)
    NEXT b
  NEXT a
  Old = 99
  Ship.X = 200
  Ship.Y = 300
  Ship.Health = 100

  FOR a = 0 TO EnemyTex1Y
    FOR b = 0 TO EnemyTex1X
      READ EnemyTex1(b, a)
    NEXT b
  NEXT a

  FOR c = 0 TO MaxNumEnemies
    Enemy1(c).Callsign = STR$(c)
    Enemy1(c).X = 250
    Enemy1(c).Y = 100
  NEXT c

  FOR a = 0 TO NumExplosionFrames
    FOR b = 0 TO ShipTexY
      FOR c = 0 TO ShipTexX
        READ ExplosionTex(a, c, b)
      NEXT c
    NEXT b
  NEXT a

  FOR a = 0 TO NumSmokeFrames
    FOR b = 0 TO ShipTexY
      FOR c = 0 TO ShipTexX
        READ SmokeTex(a, c, b)
      NEXT c
    NEXT b
  NEXT a


  REM Background Init
  FOR c = 0 TO 30
    Starfield1(c).X = INT(RND * 639) + 1
    Starfield1(c).Y = INT(RND * 479) + 1
  NEXT c
  FOR d = 0 TO 30
    Starfield2(d).X = INT(RND * 639) + 1
    Starfield2(d).Y = INT(RND * 479) + 1
  NEXT d

  REM Bullet Init
  FOR e = 0 TO 10
    Bullet(e).X = 0
    Bullet(e).Y = 0
  NEXT e

  REM ExplosionPos
  FOR n = 0 TO MaxNumExplosions
    Explosion(n).X = -5
    Explosion(n).FrameState = 0
  NEXT n
  FOR n = 0 TO MaxNumSmoke
    Smoke(n).X = -5
    Smoke(n).FrameState = 0
  NEXT n
END SUB

SUB MainMenu
  REM 1
  DO
    CLS
    COLOR 9: LOCATE 2, 4: PRINT "S"; : COLOR 1: PRINT "pace War"
    LINE (20, 32)-(98, 32), 1: LINE (23, 30)-(95, 30), 1
    LINE (20, 13)-(98, 13), 1: LINE (23, 15)-(95, 15), 1
    COLOR 9: LOCATE 4, 2: PRINT "B"; : COLOR 1: PRINT "egin"
    COLOR 9: LOCATE 5, 2: PRINT "Q"; : COLOR 1: PRINT "uit"
    Choice$ = INPUT$(1)
    Choice$ = UCASE$(Choice$)
    SELECT CASE Choice$
    CASE "B"
      CALL ShipSetup
      CALL GameLoop
      LOCATE 8, 10: PRINT "Your score was"; Score
      DO: LOOP WHILE INKEY$ = ""
    END SELECT
  LOOP UNTIL Choice$ = "Q"
END SUB

SUB PaintShip
  REM 1.1
  FOR a = 0 TO ShipTexY
    FOR b = 0 TO ShipTexX
      IF ShipTex(b, a) = Old THEN
        ShipTex(b, a) = Ship.CallCol
      END IF
    NEXT b
  NEXT a
  Old = Ship.CallCol
END SUB

SUB RenderEnemy (Index, ChangeX, ChangeY)
  FOR a = 0 TO EnemyTex1Y
    FOR b = 0 TO EnemyTex1X
      IF EnemyTex1(b, a) <> 0 THEN
        PSET (Enemy1(Index).X + b, Enemy1(Index).Y + a), 0
      END IF
    NEXT b
  NEXT a
  IF (Enemy1(Index).X + ChangeX <= 620) AND (Enemy1(Index).X + ChangeX >= 1) 
AND (Enemy1(Index).Y + ChangeY <= 460) AND (Enemy1(Index).Y + ChangeY >= 1) 
THEN
    Enemy1(Index).X = Enemy1(Index).X + ChangeX: Enemy1(Index).Y = 
Enemy1(Index).Y + ChangeY
  ELSE
    Enemy1(Index).X = INT(RND * 620) + 1: Enemy1(Index).Y = 0
    Score = Score - 1
  END IF
  FOR a = 0 TO EnemyTex1Y
    FOR b = 0 TO EnemyTex1X
      IF EnemyTex1(b, a) <> 0 THEN
        PSET (Enemy1(Index).X + b, Enemy1(Index).Y + a), EnemyTex1(b, a)
     END IF
    NEXT b
  NEXT a
END SUB

SUB RenderExplosion
  FOR c = 0 TO MaxNumExplosions
    IF Explosion(c).X <> -5 THEN
      FOR a = 0 TO ShipTexY
        FOR b = 0 TO ShipTexX
          IF ExplosionTex(Explosion(c).FrameState, b, a) <> 0 THEN
            PSET (Explosion(c).X + b, Explosion(c).Y + a), 
ExplosionTex(Explosion(c).FrameState, b, a)
          END IF
        NEXT b
      NEXT a
      IF Explosion(c).FrameState = 0 THEN
        BoomSmoke (c)
        Explosion(c).X = -5
        Explosion(c).Y = -5
        Explosion(c).FrameState = 0
      END IF
    END IF
  NEXT c
END SUB

SUB RenderHealth
  STATIC LastUpdate
  Now = TIMER
  IF (Now - LastUpdate >= .1) THEN
    LINE (5, 5)-(107, 10), 1, BF
    LINE (6, 6)-(6 + Ship.Health, 9), 4, BF
    LastUpdate = Now
  END IF
END SUB

SUB RenderShip (ChangeX, ChangeY)
  IF (Ship.X + ChangeX <= 620) AND (Ship.X + ChangeX >= 1) AND (Ship.Y + 
ChangeY <= 460) AND (Ship.Y + ChangeY >= 1) THEN
    FOR a = 0 TO ShipTexY
      FOR b = 0 TO ShipTexX
        IF ShipTex(b, a) <> 0 THEN
          PSET (Ship.X + b, Ship.Y + a), 0
       END IF
      NEXT b
    NEXT a
    Ship.X = Ship.X + ChangeX: Ship.Y = Ship.Y + ChangeY
    FOR a = 0 TO ShipTexY
      FOR b = 0 TO ShipTexX
        IF ShipTex(b, a) <> 0 THEN
          PSET (Ship.X + b, Ship.Y + a), ShipTex(b, a)
       END IF
      NEXT b
    NEXT a
  END IF
END SUB

SUB RenderShoot
  FOR a = 0 TO 30
    PSET (Bullet(a).X, Bullet(a).Y + 6), 0
    PSET (Bullet(a).X - 1, Bullet(a).Y + 7), 0
    PSET (Bullet(a).X + 1, Bullet(a).Y + 7), 0
    PSET (Bullet(a).X, Bullet(a).Y), 2
    PSET (Bullet(a).X - 1, Bullet(a).Y + 1), 10
    PSET (Bullet(a).X + 1, Bullet(a).Y + 1), 10

    PSET (Bullet(a).X + 13, Bullet(a).Y + 6), 0
    PSET (Bullet(a).X + 12, Bullet(a).Y + 7), 0
    PSET (Bullet(a).X + 14, Bullet(a).Y + 7), 0
    PSET (Bullet(a).X, Bullet(a).Y), 2
    PSET (Bullet(a).X + 12, Bullet(a).Y + 1), 10
    PSET (Bullet(a).X + 14, Bullet(a).Y + 1), 10
  NEXT a
END SUB

SUB RenderSmoke
  FOR c = 0 TO MaxNumSmoke
    IF Smoke(c).X <> -5 THEN
      FOR a = 0 TO ShipTexY
        FOR b = 0 TO ShipTexX
          IF Smoke(c).FrameState = 0 THEN
            PSET (Smoke(c).X + b, Smoke(c).Y + a), 0
          ELSE
            PSET (Smoke(c).X + b, Smoke(c).Y + a), 
SmokeTex(Smoke(c).FrameState, b, a)
          END IF
        NEXT b
      NEXT a
      IF Smoke(c).FrameState = 0 THEN
        Smoke(c).X = -5
        Smoke(c).Y = -5
        Smoke(c).FrameState = 0
      END IF
    END IF
  NEXT c
END SUB

SUB ShipSetup
  CLS
  COLOR 9: LOCATE 2, 4: PRINT "S"; : COLOR 1: PRINT "pace War"
  LINE (20, 32)-(98, 32), 1: LINE (23, 30)-(95, 30), 1
  LINE (20, 13)-(98, 13), 1: LINE (23, 15)-(95, 15), 1
  LOCATE 4, 2: COLOR 1: PRINT "Callsign: "; : INPUT "", Ship.Callsign
  LOCATE 5, 2: COLOR 1: PRINT "Ship Color:"
  COLOR 9: LOCATE 6, 4: PRINT "R"; : COLOR 4: PRINT "ed"
  COLOR 9: LOCATE 7, 4: PRINT "G"; : COLOR 2: PRINT "reen"
  COLOR 9: LOCATE 8, 4: PRINT "B"; : COLOR 9: PRINT "lue"
  Ship.CallCol = 0
  DO
    Col$ = INPUT$(1)
    Col$ = UCASE$(Col$)
    SELECT CASE Col$
    CASE "R"
      Ship.CallCol = 4
    CASE "G"
      Ship.CallCol = 2
    CASE "B"
      Ship.CallCol = 9
    END SELECT
  LOOP UNTIL Ship.CallCol <> 0
  CALL PaintShip
END SUB

SUB Shoot
  STATIC Barrel
  IF Barrel > 30 THEN
    Barrel = 0
  END IF
  Bullet(Barrel).X = Ship.X + 1
  Bullet(Barrel).Y = Ship.Y + 4
  Barrel = Barrel + 1
END SUB

SUB TimeUpdateEnemy
  STATIC LastUpdate
  Now = TIMER
  IF (Now - LastUpdate >= .1) THEN
    FOR a = 0 TO MaxNumEnemies
      CALL RenderEnemy(a, 0, 10)
    NEXT a
    LastUpdate = Now
  END IF
END SUB

SUB TimeUpdateExplosion
  STATIC LastUpdate
  Now = TIMER
  IF (Now - LastUpdate >= .1) THEN
    FOR a = 0 TO MaxNumExplosions
      IF Explosion(a).FrameState >= 0 THEN
        Explosion(a).FrameState = Explosion(a).FrameState + 1

        IF Explosion(a).FrameState > NumExplosionFrames THEN
          Explosion(a).FrameState = 0
        END IF
      END IF
    NEXT a
    CALL RenderExplosion
    LastUpdate = Now
  END IF
END SUB

SUB TimeUpdateShip
  STATIC LastUpdate
  Now = TIMER
  IF (Now - LastUpdate >= .1) THEN
    CALL RenderShip(0, 0)
    LastUpdate = Now
  END IF
END SUB

SUB TimeUpdateSmoke
  STATIC LastUpdate
  Now = TIMER
  IF (Now - LastUpdate >= .1) THEN
    FOR a = 0 TO MaxNumSmoke
      IF Smoke(a).FrameState >= 0 THEN
        Smoke(a).FrameState = Smoke(a).FrameState + 1
        IF Smoke(a).FrameState > NumSmokeFrames THEN
          Smoke(a).FrameState = 0
        END IF
      END IF
    NEXT a
    CALL RenderSmoke
    LastUpdate = Now
  END IF
END SUB

