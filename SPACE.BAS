Enemy1(p).X + EnemyTex1X) AND (Bullet(b).Y >= Enemy1(p).Y) AND (Bullet(b).Y 
<= Enemy1(p).Y + EnemyTex1Y)) THEN
            Boom (p)
            Enemy1(p).Y = 1: Enemy1(p).X = INT(RND * 620) + 1
            Score = Score + 1
          END IF
        NEXT p
      END IF
    NEXT b
    LastUpdate = Now
    CALL RenderShoot
  END IF
END SUB


